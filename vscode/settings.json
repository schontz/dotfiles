{
	"git.ignoreMissingGitWarning": true,
	"editor.wordWrap": "on",
	"terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
	"explorer.confirmDelete": false,
	"window.zoomLevel": 0,
	"editor.autoClosingBrackets": "never",
	"editor.fontSize": 16,
	"breadcrumbs.enabled": true,
	"tslint.jsEnable": true,
	"editor.insertSpaces": false,
	"editor.tabSize": 3,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/.#*": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/.#*": true
	},
	"todohighlight.isEnable": true,
	"todohighlight.keywords": [
		{
			"text": "TODO:",
			"color": "#000",
			"backgroundColor": "rgba(255,189,42,0.8)",
			"overviewRulerColor": "rgba(255,189,42,0.8)"
		},
		{
			"text": "FIXME:",
			"color": "#000",
			"backgroundColor": "rgba(240,98,146,0.8)",
			"overviewRulerColor": "rgba(240,98,146,0.8)"
		},
		{
			"text": "NOTE:",
			"color": "#eee",
			"backgroundColor": "rgba(100,100,255,0.8)",
			"overviewRulerColor": "rgba(100,100,255,0.8)"
		}
	],
	"tslint.exclude": [
		"**/node_modules/**",
		"**/dist/**"
	],
	"files.associations": {
		"*.ibas": "color-basic"
	},
	"editor.lineNumbers": "on",
	"terminal.integrated.scrollback": 250,
	"editor.accessibilitySupport": "off",
	"workbench.iconTheme": "material-icon-theme",
	"html.autoClosingTags": false,
	"window.titleBarStyle": "native",
	"files.autoGuessEncoding": true,
	"workbench.statusBar.feedback.visible": false,
	"markdown.preview.fontSize": 20,
	"printcode.fontSize": 10,
	"printcode.paperSize": "letter",
	"printcode.disableTelemetry": true,
	"vim.handleKeys": {
		"<C-d>": true,
		"<C-a>": true,
		"<C-k>": true
	},
	"search.smartCase": true,
	"window.newWindowDimensions": "inherit",
	"search.showLineNumbers": true,
	"terminal.integrated.fontWeight": "400",
	"workbench.tree.indent": 16,
	"terminal.integrated.setLocaleVariables": false,
	"typescript.locale": "en",
	"markdown-preview-enhanced.mermaidTheme": "default",
	"css.lint.emptyRules": "ignore",
	"less.lint.emptyRules": "ignore",
	"editor.fontFamily": "'Office Code Pro D', Consolas, 'Courier New', monospace",
	"editor.fontWeight": "500",
	"workbench.colorTheme": "Dark (Molokai)",
	"extensions.ignoreRecommendations": true,
